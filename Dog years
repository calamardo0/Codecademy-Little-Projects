let inputAge = prompt('What is your human age?')
var dogAge

if (inputAge > 2) 
{
  inputAge -= 2
  dogAge = 21 + 4 * inputAge
 } 
else 
{
  if (inputAge >= 0) 
  {
    dogAge = inputAge * 10.5
  } 
  else 
  {
    dogAge = 'an impossible age of'
  }
}

console.log(`You are ${dogAge} dog years old!`)

